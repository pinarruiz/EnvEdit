{
  "name": "env-edit",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "packageManager": "pnpm@8.8.0",
  "scripts": {
    "telemetry:disable": "next telemetry disable",
    "build:pre": "pnpm telemetry:disable",
    "dev": "pnpm build:pre && next dev",
    "build": "pnpm build:pre && next build",
    "start": "NODE_ENV=production next start",
    "prettier": "prettier -w .",
    "start:docker": "NODE_ENV=production pnpm build && NODE_ENV=production pnpm start",
    "lint": "next lint",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@gitbeaker/rest": "39.19.0",
    "@mantine/hooks": "7.1.2",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-slot": "1.0.2",
    "@tanstack/react-query": "4.36.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "cookies-next": "4.0.0",
    "lucide-react": "0.284.0",
    "next": "13.5.4",
    "next-auth": "4.23.2",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "1.14.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "4.36.1",
    "@tanstack/react-query-devtools": "4.36.1",
    "@types/node": "20.8.4",
    "@types/react": "18.2.27",
    "@types/react-dom": "18.2.13",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{tsx,ts,mjs,js}": [
      "eslint",
      "prettier --write"
    ],
    "*.{json,css}": [
      "prettier --write"
    ]
  }
}
