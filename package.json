{
  "name": "env-edit",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "summary:build": "node src/lib/summaries.mjs build",
    "telemetry:disable": "next telemetry disable",
    "build:pre": "pnpm summary:build && pnpm telemetry:disable && netlify --telemetry-disable",
